[sub_resource type="GDScript" id="GDScript_sbr0c"]
script/source = "extends CharacterBody2D

const SPEED = 300.0
const RUN_SPEED = 600.0  # A velocidade quando o personagem está correndo
const JUMP_VELOCITY = -400.0

# Variável de referência ao AnimatedSprite2D
var animated_sprite: AnimatedSprite2D
var punch_timer: Timer

func _ready():
	# Inicializa a variável animated_sprite com o nó AnimatedSprite2D
	animated_sprite = $AnimatedSprite2D
	
	# Inicializa o Timer de soco
	punch_timer = $PunchTimer  # Verifique se o nome do seu Timer está correto no editor
	
	# Conecta o sinal de timeout do Timer à função _on_punch_timeout corretamente
	punch_timer.connect(\"timeout\", self, \"_on_punch_timeout\")

func _physics_process(delta: float) -> void:
	# Adiciona a gravidade.
	if not is_on_floor():
		velocity += get_gravity() * delta
		# Quando o personagem está no ar, toca a animação de \"jump\".
		if animated_sprite.animation != \"jump\":
			animated_sprite.play(\"jump\")
	else:
		# Lida com o pulo.
		if Input.is_action_just_pressed(\"ui_accept\") and is_on_floor():
			velocity.y = JUMP_VELOCITY

		# Obtém a direção do movimento e lida com a movimentação/desaceleração.
		var direction := Input.get_axis(\"ui_left\", \"ui_right\")
		
		# Verifica se o jogador está apertando shift (para correr)
		var is_running = Input.is_action_pressed(\"shift\")  # Detecta quando shift é pressionado

		# Se estiver correndo, usa a velocidade RUN_SPEED
		var current_speed = SPEED  # Velocidade padrão
		if is_running:
			current_speed = RUN_SPEED  # Aumenta a velocidade para correr

		if direction:
			velocity.x = direction * current_speed
			# Se estiver se movendo, toca a animação de \"walk\" ou \"run\"
			if is_running:
				if animated_sprite.animation != \"run\":
					animated_sprite.play(\"run\")
			else:
				if animated_sprite.animation != \"walk\":
					animated_sprite.play(\"walk\")
			
			# Se mover para a esquerda, vira o personagem para a esquerda.
			if direction < 0:
				animated_sprite.flip_h = true
			else:
				animated_sprite.flip_h = false
		else:
			velocity.x = move_toward(velocity.x, 0, SPEED)
			# Se não estiver se movendo, toca a animação de \"idle\".
			if animated_sprite.animation != \"idle\":
				animated_sprite.play(\"idle\")

		# Verifica se o botão do mouse foi pressionado para a animação \"punch\".
		if Input.is_action_just_pressed(\"mouse_click\") and not punch_timer.is_active():
			# Toca a animação de \"punch\" e inicia o timer
			animated_sprite.play(\"punch\")
			punch_timer.start(0.5)  # Ajuste esse valor conforme a duração da animação de soco

	move_and_slide()

# Função que é chamada quando o timer de soco acaba
func _on_punch_timeout():
	# Após o soco, volta a permitir que o personagem execute outras animações
	if not Input.is_action_pressed(\"shift\"):  # Se não estiver pressionando shift, retorna a animação \"idle\"
		if animated_sprite.animation != \"idle\":
			animated_sprite.play(\"idle\")
"

